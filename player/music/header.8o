:macro loadSong m delayStart {
  v0 := m  songLoadAddresses
	:calc z { 255 - delayStart }
	v0 := z  i := tickStates  save v0
}

:macro |> k { :pointer k }
:macro buzzerpitch vX {  :byte  { 0xF0 + vX }  0x3A  }

:macro mySongList {
